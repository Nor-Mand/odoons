<?xml version="1.0" encoding="utf-8" ?>
<odoo>
<!--  Search view  -->
    <record id="solicitud_boletas_view_search" model="ir.ui.view">
        <field name="name">solicitud.boletas.view.search</field>
        <field name="model">solicitud.garantias</field>
        <field name="inherit_id" ref="hexacorp_garantias.solicitud_boletas_view_search"/>
        <field name="arch" type="xml">
            <xpath expr="//search/group" position="inside">
                <filter string="My Boletas" name="my_boletas" domain="[('create_uid','=', uid)]"/>
            </xpath>

        </field>
    </record>
</odoo>